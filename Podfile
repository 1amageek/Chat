# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Chat' do
  use_frameworks!

  # Pods for Chat
  pod 'RealmSwift'

  pod 'Firebase'
  pod 'Firebase/Messaging'
  pod 'Firebase/Database'
  pod 'Firebase/Crash'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'

end

target 'TwitterMessenger' do
  use_frameworks!

  # Pods for Chat
  pod 'RealmSwift'
  pod 'APIKit', '~> 3.0'

end
